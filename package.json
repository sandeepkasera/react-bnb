{
  "name": "web-bnb-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently --names client,api \"npm:dev\" \"npm:api\"",
    "api": "nodemon server/server.js",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "report": "npm-run-all --sequential --continue-on-error --silent create-report open-report",
    "create-report": "vitest run --reporter=html",
    "open-report": "vite preview --outDir html"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "lodash.isequal": "^4.5.0",
    "rc-slider": "^11.1.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "react-select": "^5.10.1",
    "react-toggle": "^4.1.3",
    "sass": "^1.87.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/ui": "^3.1.1",
    "concurrently": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "swagger-ui-express": "^4.1.6",
    "vite": "^6.2.6",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=10.12.0",
    "npm": ">=6.0.0"
  }
}
